<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="source.controller.gui_controllers.SelectProgramController">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="800.0" HBox.hgrow="ALWAYS">
         <children>
            <BorderPane layoutX="200.0" layoutY="100.0" prefHeight="600.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <center>
                  <ListView fx:id="programsList" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
               </center>
               <bottom>
                  <Button fx:id="selectButton" mnemonicParsing="false" onAction="#selectButtonHandler" text="Select" BorderPane.alignment="CENTER">
                     <font>
                        <Font name="Times New Roman" size="15.0" />
                     </font></Button>
               </bottom>
            </BorderPane>
         </children>
      </AnchorPane>
   </children>
</HBox>
